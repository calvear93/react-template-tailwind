{
    "default": {
        "HTTPS": false,
        "GENERATE_SOURCEMAP": true,
        "REACT_APP_SERVICE_WORKER": false,
        "REACT_APP_BASE_PATH": "/",
        "REACT_APP_NO_SCRIPT": "You need to enable JavaScript to run this app."
    },
    "mode": {
        "build": {
            "NODE_ENV": "production",
            "SKIP_PREFLIGHT_CHECK": false,
            "IMAGE_INLINE_SIZE_LIMIT": 0,
            "REACT_APP_SERVICE_WORKER": false,
            "REACT_APP_BASE_PATH": "/<base-path>",
            "REACT_APP_DEBUG": false
        },
        "debug": {
            "NODE_ENV": "development",
            "PUBLIC_URL": ".",
            "REACT_APP_DEBUG": true,
            "REACT_APP_BASE_PATH": "/",
            "PORT": 3001
        },
        "test": {
            "NODE_ENV": "test",
            "REACT_APP_DEBUG": true
        }
    },
    "env": {
        "dev": {
            "PUBLIC_URL": "https://<app-url-dev>/<base-path>"
        },
        "qa": {
            "PUBLIC_URL": "https://<app-url-qa>/<base-path>"
        },
        "prod": {
            "PUBLIC_URL": "https://<app-url-prod>/<base-path>",
            "GENERATE_SOURCEMAP": false
        }
    }
}
